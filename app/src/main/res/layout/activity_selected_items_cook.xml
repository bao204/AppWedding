<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".admin.selectedLobby_itemActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="48dp"
        android:orientation="horizontal"
        android:gravity="center"
        android:layout_gravity="center"
        android:id="@+id/seclectedCookItemActivity_ViewgroupToolbar">

        <include layout="@layout/icon_back_toolbar_notbackground"
            android:id="@+id/clickBack_selectedCookActivity"/>

        <include layout="@layout/toolbar_default"
            android:layout_width="290dp"
            android:layout_height="40dp"
            android:layout_margin="7dp"
            android:id="@+id/toolbar_selectedCookActivity"
            app:menu="@menu/selected"/>

    </LinearLayout>

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/recyclerSelectedCookActivity"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="4dp"
        android:layout_below="@+id/seclectedCookItemActivity_ViewgroupToolbar"/>

</RelativeLayout>